<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/icon_media_route_on" android:state_checked="true" android:state_enabled="true" />
    <item android:drawable="@drawable/anim_cast_button_connecting" android:state_checkable="true" android:state_enabled="true" />
    <item android:drawable="@drawable/icon_media_route_off" android:state_enabled="true" />
